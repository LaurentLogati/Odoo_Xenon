<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="account_fr_fec_view_xenon" model="ir.ui.view">
    <field name="name">account.fr.fec.form.xenon</field>
    <field name="model">account.fr.fec</field>
    <field name="inherit_id" ref="l10n_fr_fec.account_fr_fec_view"/>
    <field name="arch" type="xml">
    <xpath expr="//button[@name='generate_fec']" position="replace">  
        <button name="generate_fec_xenon" string="Generate" type="object" class="oe_highlight"/> 
        <button name="generate_export_cabinet_xenon" string="Export cabinet" type="object" class="oe_highlight"/>       
    </xpath> 
    </field>
</record>
    
   
<record id="export_cabinet_action" model="ir.actions.act_window">
    <field name="name">Export Cabinet</field>
    <field name="res_model">account.fr.fec</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
</record>
    
<menuitem id="export_cabinet_menu"
        parent="l10n_fr.account_reports_fr_statements_menu"
        action="export_cabinet_action"/>

</odoo>